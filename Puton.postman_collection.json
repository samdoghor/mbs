					"name": "create_season",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Parse the provided dates into JavaScript Date objects\r",
									"const startDate = new Date(\"2014-08-16\");\r",
									"const endDate = new Date(\"2015-05-24\");\r",
									"\r",
									"// Format the dates as YYYY-MM-DD\r",
									"const startDateString = startDate.toISOString().split('T')[0];\r",
									"const endDateString = endDate.toISOString().split('T')[0];\r",
									"\r",
									"// Set the date strings as environment variables in Postman\r",
									"pm.environment.set('start_date', startDateString);\r",
									"pm.environment.set('end_date', endDateString);"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
